{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "jsx": "preserve",
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "@types/node",
      "@types/canvas-confetti",
      "@nuxt/types",
      "@nuxt/content",
      "@nuxtjs/i18n",
      "@nuxtjs/axios",
      "@nuxtjs/vuetify",
      "@pinia/nuxt",
      "unplugin-vue2-script-setup/types"
    ]
  },
  "vueCompilerOptions": {
    "experimentalCompatMode": 2
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist"
  ]
}
